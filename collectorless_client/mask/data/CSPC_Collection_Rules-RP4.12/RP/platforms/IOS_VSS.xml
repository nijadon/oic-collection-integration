<Platform identifier="_IOS_VSS">
<Title>IOS_VSS</Title>
<Description>IOS Switches with VSS Enabled</Description>
<MatchType>ALL</MatchType>
<PlatformConditionList>
  <PlatformCondition>
    <PropertyName>SNMP_OID</PropertyName>
    <Operator>IN_LIST</Operator>
    <ValueList>
      <Value>.1.3.6.1.4.1.9.1.1732</Value>
      <Value>.1.3.6.1.4.1.9.1.1934</Value> 
      <Value>.1.3.6.1.4.1.9.1.896</Value>
    </ValueList>
  </PlatformCondition>
  <PlatformCondition>
      <PropertyName>SNMP_SYS_DESCR</PropertyName>
      <Operator>DOESNOT_MATCH_REGULAR_EXPRESSION</Operator>
      <ValueList>
          <Value><![CDATA[.*IOS-XE.*]]></Value>
      </ValueList>
  </PlatformCondition>
</PlatformConditionList>
</Platform>