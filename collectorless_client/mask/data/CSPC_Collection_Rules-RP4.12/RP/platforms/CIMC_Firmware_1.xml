<Platform identifier="_CIMC_Firmware_1">
  <Title>CIMC_Firmware_1</Title>
  <Description>UCS C-Series Servers using same OID as UCS FI</Description>
  <Locked>false</Locked>
  <MatchType>ALL</MatchType>
  <PlatformConditionList>
    <PlatformCondition>
      <PropertyName>SNMP_OID</PropertyName>
      <Operator>IN_LIST</Operator>
      <ValueList>
        <Value>.1.3.6.1.4.1.9.12.3.1.3.847</Value>
      </ValueList>
    </PlatformCondition>
    <PlatformCondition>
      <PropertyName>SNMP_SYS_DESCR</PropertyName>
      <Operator>DOESNOT_MATCH_REGULAR_EXPRESSION</Operator>
      <ValueList>
        <Value>.*Cisco NX-OS.*</Value>
      </ValueList>
    </PlatformCondition>
  </PlatformConditionList>
</Platform>